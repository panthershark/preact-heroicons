{
  "name": "preact-heroicons",
  "version": "2.0.13",
  "description": "Heroicons as Preact components",
  "type": "module",
  "types": "./dist/index.d.mts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.mts"
    }
  },
  "sideEffects": false,
  "scripts": {
    "build": "npm run generate && npm run transpile",
    "checkversion": "node scripts/checkversion.js",
    "clean": "rm -rf dist && rm -rf gen",
    "format": "prettier --write \"./**/*.{js,jsx,ts,tsx,mjs,mts}\"",
    "generate": "node build.js && npm run format",
    "test": "vitest",
    "transpile": "tsc",
    "publish": "npm publish --access public --publish-branch preact-heroicons",
    "postinstall": "npm run generate && npm run transpile"
  },
  "repository": "github:ekwoka/preact-heroicons",
  "keywords": [
    "heroicons",
    "svg",
    "preact",
    "icons",
    "icon"
  ],
  "author": {
    "name": "Eric Kwoka",
    "email": "ninjatheory@gmail.com",
    "url": "https://thekwoka.net/"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ekwoka/preact-heroicons/issues"
  },
  "publishConfig": {
    "access": "public"
  },
  "homepage": "https://github.com/ekwoka/preact-heroicons#readme",
  "files": [
    "."
  ],
  "devDependencies": {
    "@preact/preset-vite": "^2.4.0",
    "@testing-library/preact": "^3.2.2",
    "enzyme": "^3.11.0",
    "enzyme-adapter-preact-pure": "^4.0.1",
    "happy-dom": "^7.7.0",
    "preact": "^10.11.3",
    "prettier": "^2.8.0",
    "typescript": "^4.9.3",
    "vite": "^3.2.3",
    "vitest": "^0.25.3"
  },
  "peerDependencies": {
    "preact": "^10.5.0"
  }
}
